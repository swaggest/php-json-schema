    "codeclimate/php-test-reporter": "0.1.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Viacheslav Poturaev",
      "email": "vearutop@gmail.com"
    }
  ],
  "support": {
    "email": "vearutop@gmail.com"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Swaggest\\JsonSchema\\": "src/"
